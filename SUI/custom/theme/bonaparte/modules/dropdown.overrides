/*******************************
         Theme Overrides
*******************************/

/* Dropdown Carets */
@font-face {
  font-family: 'Dropdown';
  src:
    url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype'),
    url(data:application/font-woff;charset=utf-8;base64,) format('woff')
  ;
  font-weight: normal;
  font-style: normal;
}

.ui.dropdown > .dropdown.icon {
  font-family: 'Dropdown';
  line-height: 1;
  height: 1em;
  width: 1.23em;
  backface-visibility: hidden;
  font-weight: normal;
  font-style: normal;
  text-align: center;
}

.ui.dropdown > .dropdown.icon {
  width: auto;
}
.ui.dropdown > .dropdown.icon:before {
  content: '\f0d7';
}

/* Sub Menu */
.ui.dropdown .menu {
  left: 1px;
  width: ~"calc(100% - 2px)";
  border: none;
  background-color: @color-light-1;
  box-shadow: 0 1px 1px 0 rgba(34,36,38,.15);
}

.ui.dropdown .menu .item .dropdown.icon:before {
  content: '\f0da'/*rtl:'\f0d9'*/;
}

.ui.dropdown .item .left.dropdown.icon:before,
.ui.dropdown .left.menu .item .dropdown.icon:before {
  content: "\f0d9"/*rtl:"\f0da"*/;
}

/* Vertical Menu Dropdown */
.ui.vertical.menu .dropdown.item > .dropdown.icon:before {
  content: "\f0da"/*rtl:"\f0d9"*/;
}

/* Icons for Reference */
.dropdown-down.icon:before {
  font-family: 'Dropdown';
  content: "\f0d7";
}
.dropdown-up.icon:before {
  font-family: 'Dropdown';
  content: "\f0d8";
}
.dropdown-left.icon:before {
  font-family: 'Dropdown';
  content: "\f0d9";
}
.dropdown-icon.icon:before {
  font-family: 'Dropdown';
  content: "\f0da";
}

.ui.selection.dropdown {
  padding-right: 6em;
}

.ui.selection.dropdown.dark,
.ui.selection.dropdown.inverted {
  color: @color-light-0;
  background-color: transparent;
}

.ui.selection.dropdown > .search.icon,
.ui.selection.dropdown > .delete.icon,
.ui.selection.dropdown > .dropdown.icon {
    color: @color-interactive-light;
    height: 4rem;
    width: 4rem;
}

.ui.selection.dropdown.active > .search.icon,
.ui.selection.dropdown.active > .delete.icon,
.ui.selection.dropdown.active > .dropdown.icon {
  color: @color-light-0;
  background-color: @color-active;
}
.ui.selection.dropdown.active > .dropdown.icon {
  transform: rotate(180deg);
}

.ui.selection.dropdown.dark .menu,
.ui.selection.dropdown.inverted .menu {
  background-color: @color-dark-1;
  border-bottom: 3px solid @color-active !important; 
}

.ui.selection.dropdown.inverted.active .text {
  color: @color-active-light;
  font-weight: @selectionVisibleTextFontWeight;
}

.ui.selection.dropdown.inverted .menu > .item {
  color: @color-light-0;
  border: 0;
}
.ui.selection.dropdown.inverted .menu > .item:hover {
  background-color: @color-dark-0;
}
.ui.selection.dropdown.inverted .menu > .item.active {
  color: @color-active;
}